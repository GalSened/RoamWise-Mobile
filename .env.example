# Google Maps Platform
VITE_GOOGLE_MAPS_API_KEY=
VITE_GOOGLE_PLACES_ENABLED=true
VITE_GOOGLE_ROUTES_ENABLED=true
VITE_GOOGLE_ELEVATION_ENABLED=true

# Routing (fallbacks)
VITE_ROUTING_PROVIDER=google
VITE_ROUTING_BASE_URL=

# Weather
VITE_WEATHER_PROVIDER=openweather
VITE_OPENWEATHER_API_KEY=
VITE_WEATHERKIT_TEAM_ID=
VITE_WEATHERKIT_KEY_ID=
VITE_WEATHERKIT_PRIVATE_KEY_BASE64=

# Voice / AI (optional)
VITE_STT_PROVIDER=system
VITE_LLM_PROVIDER=none
VITE_LLM_API_KEY=

# Map styles
VITE_MAP_STYLE_LIGHT=maplibre-light
VITE_MAP_STYLE_DARK=maplibre-dark

# Backend
VITE_API_BASE_URL=http://localhost:8080

# Telemetry
VITE_TELEMETRY_ENABLED=true
VITE_TELEMETRY_ENDPOINT=/api/telemetry